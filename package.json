{
  "name": "samjs-mongo",
  "description": "Adds a model and interface to samjs to connect with mongoDB",
  "version": "0.2.1",
  "repository": "https://github.com/SAMjs/samjs-mongo",
  "author": {
    "name": "Paul Pflugradt",
    "email": "paul.pflugradt@gmail.com"
  },
  "main": "lib/main.js",
  "engines": {
    "node": "*"
  },
  "dependencies": {
    "mongoose": "^4.6.3"
  },
  "devDependencies": {
    "atom-ui-reporter": "0.0.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.11.1",
    "mocha": "^3.1.2",
    "nodemon": "^1.11.0",
    "pug": "^2.0.0-beta6",
    "rimraf": "^2.5.4",
    "samjs": "^0.2.3",
    "samjs-client": "^0.2.2",
    "script-runner": "^0.1.5",
    "vue-compiler": "^1.0.0",
    "vue-html-loader": "^1.2.3"
  },
  "keywords": [],
  "readmeFilename": "README.md",
  "scripts": {
    "build": "run-npm build:*",
    "build:coffee": "coffee --no-header --compile --output lib/ src/*.coffee",
    "build:vue": "vue-compiler --out lib/ src/*.vue",
    "dev": "nodemon --watch src --watch dev dev/server.coffee",
    "test": "mocha",
    "preversion": "npm test",
    "version": "npm run build && git add .",
    "postversion": "git push && git push --tags && npm publish"
  }
}
